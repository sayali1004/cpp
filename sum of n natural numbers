//sum of n natural numbers

#include <stdio.h>
int sum(int n)
{   if(n==0)
      return 0;
    else
    return sum(n-1)+n;
}

int sum1(int n)
{
    int i;
    int S=0;
    for(i=1;i<=n;i++)
      
          S=S+i;
          return S;
      
     
}


int main()
{   int s;
    s=sum1(5);
    printf("%d",s );

    return 0;
}
